"#MainWindow": {
	backgroundColor: "#EEE",
},
"#MainWindow[platform=android]": {
	navBarHidden: true,
	exitOnClose: true
},
"#MainWindow[platform=android formFactor=tablet]": {
	orientationModes: [
		Ti.UI.LANDSCAPE_LEFT
	]
},
"#MainWindow[formFactor=handheld]": {
	orientationModes: [
		Ti.UI.PORTRAIT
	]
},
"#GlobalWrapper": {
	width: "100%"
},
"#ContentWrapper": {
	top: "0dp",
	bottom: "60dp"
}